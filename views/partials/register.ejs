<body>
      <main class="col-md-9 ms-sm-auto col-lg-10 px-md-4 transition-fade login-main">
        <div
            class="login-controller">
            <div class="login-bg"></div>
        <form class="form-container" action="/register" method="post">
            <h1 class="signin-header">Café Login</h1>
        
            <div class="form-floating input-field">
              <input type="text" class="form-control" id="floatingInput" placeholder="name@example.com" name="getRegisterUsername" autocomplete="off">
              <label>Username</label>
            </div>
            <div class="form-floating input-field">
              <input type="password" class="form-control" id="floatingPassword" placeholder="Password" name="getRegisterPassword">
              <label for="floatingPassword">Password</label>
            </div>
            <button class="w-100 btn btn-lg login-btn register-btn" type="submit">Register</button>
            <p class="mt-3 mb-2 text-muted" style="text-align: center;">© Café Management 2021-22</p>
          </form>
          </div>
        </div>
    </main>

  <%- include("../partials/footer"); -%>